{
  "name": "sso-bot-sample",
  "version": "1.0.0",
  "description": "Microsoft 365 Agents Toolkit sso bot sample",
  "engines": {
    "node": "18 || 20 || 22"
  },
  "author": "Microsoft",
  "license": "MIT",
  "main": "./lib/src/index.js",
  "scripts": {
    "dev:teamsfx": "env-cmd --silent -f .localConfigs npm run dev",
    "dev": "nodemon --exec node --inspect=0.0.0.0:9239 --signal SIGINT -r ts-node/register ./src/index.ts",
    "build": "tsc --build && shx cp -r ./public ./lib",
    "start": "node ./lib/src/index.js",
    "watch": "nodemon --exec \"npm run start\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com"
  },
  "dependencies": {
    "@microsoft/microsoft-graph-client": "^3.0.1",
    "@microsoft/teamsfx": "^3.0.0-alpha",
    "botbuilder": "^4.23.1",
    "botbuilder-dialogs": "^4.23.1",
    "isomorphic-fetch": "^3.0.0",
    "express": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^18.0.0",
    "@types/json-schema": "^7.0.15",
    "env-cmd": "^10.1.0",
    "ts-node": "^10.4.0",
    "typescript": "^5.3.0",
    "nodemon": "^3.1.7",
    "shx": "^0.3.3"
  }
}
