{
    "name": "stocks-update-notification-bot",
    "version": "1.0.0",
    "description": "Stocks Update Notification bot",
    "engines": {
        "node": ">=14 <=16"
    },
    "author": "Microsoft",
    "license": "MIT",
    "scripts": {
        "dev:teamsfx": "node teamsfx/script/run.js . teamsfx/.env.local",
        "dev": "func start --typescript --language-worker=\"--inspect=9239\" --port \"3978\" --cors \"*\"",
        "prepare-storage:teamsfx": "azurite --silent --location ./_storage_emulator --debug ./_storage_emulator/debug.log",
        "watch:teamsfx": "tsc --watch",
        "build": "tsc && shx cp -r ./src/adaptiveCards ./dist",
        "watch": "tsc -w",
        "prestart": "npm run build",
        "start": "npx func start",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com"
    },
    "dependencies": {
        "@microsoft/adaptivecards-tools": "^1.0.0",
        "@microsoft/teamsfx": "^2.0.0",
        "botbuilder": "~4.15.0",
        "lodash": "^4.17.21"
    },
    "devDependencies": {
        "@azure/functions": "^1.2.3",
        "@microsoft/teamsfx-run-utils": "alpha",
        "@types/lodash": "^4.14.182",
        "azurite": "^3.16.0",
        "shx": "^0.3.4",
        "ts-node": "~9.1.1",
        "typescript": "~3.9.2"
    }
}
