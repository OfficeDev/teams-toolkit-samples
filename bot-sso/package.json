    "@microsoft/agents-hosting-teams": "^0.4.1",
    "jwt-decode": "^4.0.0",
    "@azure/identity": "^4.4.1",
    "@azure/msal-node": "^2.14.0",
    "isomorphic-fetch": "^3.0.0",
    "express": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^18.0.0",
    "@types/json-schema": "^7.0.15",
    "env-cmd": "^10.1.0",
